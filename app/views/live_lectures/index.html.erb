<p id="notice"><%= notice %></p>

<h1>Live Lectures</h1>

<table>
  <thead>
    <tr>
      <th>Lecture</th>
      <th>Question</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @live_lectures.each do |live_lecture| %>
      <tr>
        <td><%= live_lecture.lecture %></td>
        <td><%= live_lecture.question %></td>
        <td><%= live_lecture.state %></td>
        <td><%= link_to 'Show', live_lecture %></td>
        <td><%= link_to 'Edit', edit_live_lecture_path(live_lecture) %></td>
        <td><%= link_to 'Destroy', live_lecture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Live Lecture', new_live_lecture_path %>
